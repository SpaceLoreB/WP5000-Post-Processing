function out = printOverallResTable_DE(outStr,cfgName)
% The input structure "config" contains:
% - parameters (h_w, v_min)

if nargin == 2
    fname = ['results/' cfgName '_OverallsTableDE.txt'];
elseif nargin == 1
    fname = ['results/' inputname(1) '_OverallsTableDE.txt'];
else
    disp('Bro WTF');
    return
end

%% % thresholds for acceptable results. Copied from OEM report.
%% % to-do: define as input, change them based on reporting requirements
%thrQ_u_tot = 60;    % percent usable wrt total flow
%thr_DeltaQ_u = 15;  % max L/R divergence
%thr_CV_u = 35;       % max CV
%thr_outliers = 30;
%thr_Q_NU2wh = 20;
%thr_Q_NUOverWH = 15;
%thr_Q_NU2hihg = 5;

% % Creating a txt file, putting some basic headers
fileID = fopen(fname,'w');
fprintf(fileID,'\\hline\n');
fprintf(fileID, ' & \t Links \t&\t\\%% $Q_{tot}$\t&\t Rechts \t&\t\\%% $Q_{tot}$\\\\ \n');
fprintf(fileID,'\\hline\n');

% pre-formatting lines
formatSpecPerc = '%s \t&\t \\multicolumn{2}{c|}{%.1f\\%%} \t&\t \\multicolumn{2}{c|}{%.1f\\%%} \\\\ \n'; 
formatSpecNoThr = '%s \t&\t %.0f \t&\t %.1f\\%% \t&\t %.0f \t&\t %.1f\\%%\\\\ \n';
formatSpecNormal = '%s \t&\t %.0f \t&\t %.1f\\%% \t&\t %.0f \t&\t %.1f\\%% \\\\ \n';

% Print Qtot
fprintf(fileID,'%s \t&\t \\multicolumn{2}{c|}{%.0f} \t&\t \\multicolumn{2}{c|}{%.0f} \\\\ \n','Luftvolumen insgesamt $Q_{tot}$ [\unit{\cubic m\per h}]',outStr.results.Qtot(1),outStr.results.Qtot(2));
fprintf(fileID,'%s \t&\t \\multicolumn{4}{c|}{%.0f} \\\\ \n','Gesamt [\unit{\cubic m\per h}]', sum(outStr.results.Qtot) );
fprintf(fileID,'%s \t&\t \\multicolumn{4}{c|}{%.1f \\%%} \\\\ \n','Differenz L/R', abs((outStr.results.Qtot(1) - outStr.results.Qtot(2)) / outStr.results.Qtot(1))*100 );

% Q_Usable and related stats (CV, outliers)
fprintf(fileID,'\\hline\n');
fprintf(fileID,formatSpecNoThr, sprintf('Nutzbares Luftvolumen ($\\ge$ %.1f m/s) bis zur %.1f m',outStr.params(2),outStr.params(1)/100), outStr.results.Q_Usable2hw_tot(1),outStr.results.Q_Usable2hw_perc(1), outStr.results.Q_Usable2hw_tot(2),outStr.results.Q_Usable2hw_perc(2) );
fprintf(fileID,'%s \t&\t \\multicolumn{4}{c|}{%.0f (%.1f\\%%)} \\\\ \n','Gesamt nutzbar ([\unit{\cubic m\per h}], \% $Q_{tot}$) ', sum(outStr.results.Q_Usable2hw_tot), sum(outStr.results.Q_Usable2hw_tot)/sum(outStr.results.Qtot)*100);
fprintf(fileID,'%s \t&\t \\multicolumn{4}{c|}{%.1f \\%%}\\\\ \n','Differenz L/R', 100*abs((outStr.results.Q_Usable2hw_tot(1) - outStr.results.Q_Usable2hw_tot(2)) / outStr.results.Q_Usable2hw_tot(1)));

fprintf(fileID,formatSpecPerc,'Schwankungen des nutzbaren Luftvolumens  (VK \%)	',outStr.results.Q_Us_muStdCV(3,1),outStr.results.Q_Us_muStdCV(3,2));
fprintf(fileID,formatSpecPerc,'Ausreißer bei der mittleren nutzbaren Luftmenge  [\%]	',outStr.results.Q_outliers(1),outStr.results.Q_outliers(2));

% Potentially usable (slightly above, top)
fprintf(fileID,'\\hline\n');

fprintf(fileID,formatSpecNormal, sprintf('Nicht-nutzbares Luftvolumen über %.1f m [\unit{\cubic m\per h}]',outStr.params(1)/100)', outStr.results.Q_NonUsable2hihg_tot(1),outStr.results.Q_NonUsable2hihg_perc(1), outStr.results.Q_NonUsable2hihg_tot(2),outStr.results.Q_NonUsable2hihg_perc(2));

fprintf(fileID,'\\hline\n');

% % We're done
out = fclose(fileID);
disp('Table printed');
end
